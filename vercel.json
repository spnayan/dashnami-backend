{
    "builds": [{
        "src": "backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ],
    "buildCommand": "pip install -r requirements.txt && python manage.py migrate"

}