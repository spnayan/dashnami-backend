{
    "version": 2,
    "builds": [
      {
        "src": "backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        
      },
      {
        "src": "deploy.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "backend/static"
        }
      }
    ],
    "routes": [
      {
        "src": "/staticfiles/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "backend/wsgi.py"
      }
    ],
    "buildCommand":" echo 'hello' "
  }
